#include <stdio.h>

int isPrime(int num) {
    if (num < 2) return 0;
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) return 0;
    }
    return 1;
}

void printPrimesUpToN(int n) {
    for (int i = 1; i <= n; i++) {
        if (isPrime(i)) printf("%d ", i);
    }
}

int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);
    printPrimesUpToN(n);
    return 0;
}
